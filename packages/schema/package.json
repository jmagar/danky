  },
  "dependencies": {
    "@h1deya/langchain-mcp-tools": "^0.1.4",
    "langchain": "0.3.11",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@danky/eslint-config": "workspace:*",
    "@danky/typescript-config": "workspace:*",
    "eslint": "^9.18.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.4.5"
  }
